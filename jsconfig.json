{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "AppAssets/*": ["./src/assets/*"],
      "AppTheme": ["./src/theme/index.js"],
      "AppTheme/*": ["./src/theme/*"],
      "AppFirebase": ["./src/firebase/index.js"],
      "AppFirebase/*": ["./src/firebase/*"],
      "AppNetwork": ["./src/network/index.js"],
      "AppNetwork/*": ["./src/network/*"],
      "AppUtils": ["./src/utils/index.js"],
      "AppUtils/*": ["./src/utils/*"],
      "AppConstants": ["./src/constants/index.js"],
      "AppConstants/*": ["./src/constants/*"],
      "AppComponents": ["./src/components/index.js"],
      "AppComponents/*": ["./src/components/*"],
      "AppNavigation": ["./src/navigation/index.js"],
      "AppNavigation/*": ["./src/navigation/*"],
      "AppContainers": ["./src/containers/index.js"],
      "AppContainers/*": ["./src/containers/*"],
      "AppActions": ["./src/actions/index.js"],
      "AppActions/*": ["./src/actions/*"],
      "AppReducers": ["./src/reducers/index.js"],
      "AppReducers/*": ["./src/reducers/*"],
      "AppStore": ["./src/store/index.js"],
      "AppStore/*": ["./src/store/*"],
      "AppModels": ["./src/models/index.js"],
      "AppModels/*": ["./src/models/*"],
      "AppConfig/*": ["./app.json"]

    },
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES6"
  }
}